{
  "name": "@dolinho-bot/bot",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@dolinho/slash": "latest",
    "@dolinho/market": "latest",
    "@dolinho/types": "latest",
    "@dolinho/utils": "latest"
  },
  "nx": {
    "name": "bot",
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "platform": "node",
          "outputPath": "apps/bot/dist",
          "format": [
            "cjs"
          ],
          "bundle": false,
          "main": "apps/bot/src/main.ts",
          "tsConfig": "apps/bot/tsconfig.app.json",
          "assets": [
            "apps/bot/src/assets"
          ],
          "esbuildOptions": {
            "sourcemap": true,
            "outExtension": {
              ".js": ".js"
            }
          }
        },
        "configurations": {
          "development": {},
          "production": {
            "esbuildOptions": {
              "sourcemap": false,
              "outExtension": {
                ".js": ".js"
              }
            }
          }
        }
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "bot:build",
          "runBuildTargetDependencies": true
        },
        "configurations": {
          "development": {
            "buildTarget": "bot:build:development"
          },
          "production": {
            "buildTarget": "bot:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  }
}
